{
  "name": "bun-test-express-ts",
  "module": "./src/server.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun --watch --smol --hot ./src/server.ts",
    "start": "bun ./src/server.ts",
    "bundle": "bun build ./src/server.ts --outdir ./dist --target bun",
    "type-check": "tsc --noEmit",
    "type-watch": "tsc --noEmit -w"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/bun": "latest",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mongoose": "^5.11.97",
    "@types/mongoose-aggregate-paginate-v2": "^1.1.2",
    "@types/multer": "^2.0.0",
    "@types/node": "^25.0.10"
  },
  "peerDependencies": {
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cloudinary": "^2.9.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.6",
    "express": "^5.2.1",
    "jsonwebtoken": "^9.0.3",
    "mongoose": "^9.1.5",
    "mongoose-aggregate-paginate-v2": "^1.1.4",
    "multer": "^2.0.2"
  }
